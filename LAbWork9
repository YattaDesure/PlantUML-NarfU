@startuml
left to right direction
class Пользователь {
 +id (PK)
 +номер телефона: integer
 +имя: string
 +фамилия: string
 +дата рождения: Date
}
class Покупка_товара{
+id (PK)
+Сотрудник: string
+Пользователь: string
+Товар: string
+Дата операции: Date
}


class Сотрудник {
 +id (PK)
 +номер телефона: integer
 +фамилия: string
 +имя: string
 +дата рождения: Date
}
class Товар {
 +id (PK)
 +название товара: string
 +номер товара: integer
 +дата добавления товара: date
 +количество товара: integer
}
Сотрудник "1" -- "n" Покупка_товара : оформляет
Пользователь "1" -- "n" Покупка_товара : совершает
Товар "n" -- "m" Покупка_товара : входит в

@enduml
